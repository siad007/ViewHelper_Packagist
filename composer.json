{
	"name": "siad007/packagist",
	"description": "Packagist API Client and View-Helper-Collection for Zend Framework 2",
	"license": "BSD 3-Clause",
	"type": "library",
	"keywords": ["zf2", "zendframework"],
	"homepage": "http://siad007.github.io/ViewHelper_Packagist/",
	"authors": [
		{
			"name": "Siad Ardroumli",
			"email": "siad.ardroumli@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"zendframework/zendframework": "2.*"
	},
	"require-dev": {
	        "satooshi/php-coveralls": "dev-master"
	},
	"suggest": {
        	"zendframework/skeleton-application": "Create a standalone packagist client out of the box."
    	}
	"autoload": {
                "psr-0": {
                        "ViewHelper_Packagist": "src/",
                        "ViewHelper_PackagistTest": "test/ViewHelper_PackagistTest"
                },
                "classmap": [
                        "./"
                ]
	}
}
